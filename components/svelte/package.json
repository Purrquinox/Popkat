{
	"name": "@purrquinox/popkat",
	"version": "1.0.6",
	"description": "Hello, there. This is the Popkat CDN component for Sveltekit. This component simplifies uploading files to AWS S3 using our API, which can be easily self-hosted by following the instructions on our Github!",
	"private": false,
	"sideEffects": false,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		"./upload.svelte": {
			"types": "./package/upload.svelte.d.ts",
			"svelte": "./package/upload.svelte"
		}
	},
	"files": [
		"package",
		"!package/**/*.test.*",
		"!package/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@sveltejs/package": "^1.0.0",
		"autoprefixer": "^10.4.18",
		"postcss": "^8.4.36",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.0.0",
		"@skeletonlabs/skeleton": "^2.6.1",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@fortawesome/fontawesome-free": "^6.5.1",
		"@tailwindcss/forms": "^0.5.7",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.9.0"
	},
	"svelte": "./package/index.js",
	"types": "./package/index.d.ts",
	"type": "module"
}
